{
  "name": "resource-service",
  "version": "0.0.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run cp",
    "cp": "cp README.md LICENSE dist/resource",
    "publish": "npm run build && cd dist/resource && npm publish",
    "test": "ng test",
    "coverage": "ng test --watch=false --code-coverage && npm run serve:coverage",
    "serve:coverage": "pushstate-server ./coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": ">=6.0",
    "@angular/common": ">=6.0",
    "@angular/compiler": ">=6.0",
    "@angular/core": ">=6.0",
    "@angular/forms": ">=6.0",
    "@angular/platform-browser": ">=6.0",
    "@angular/platform-browser-dynamic": ">=6.0",
    "@angular/router": ">=6.0",
    "core-js": ">2.6.2",
    "rxjs": ">=6.0",
    "zone.js": ">=0.8 <0.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": ">=0.12",
    "@angular-devkit/build-ng-packagr": ">=0.10",
    "@angular/cli": ">=6.0",
    "@angular/compiler-cli": ">=6.0",
    "@angular/language-service": ">=6.0",
    "@types/jasmine": "^3.4.2",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": ">4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": ">=10.1.1",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.10.0",
    "protractor": "^5.4.2",
    "pushstate-server": "^3.0.1",
    "ts-node": "^8.4.1",
    "tsickle": ">0.34.0",
    "tslib": "^1.9.0",
    "tslint": "^5.20.0",
    "typescript": ">=3.4 <3.6"
  }
}
